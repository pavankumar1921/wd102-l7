<table border="1px">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Due Date</th>
        <th>Is Completed</th>
    </tr>
    <% for(var i=0; i < allTodos.length; i++){ %>
    <tr>
        <td><%= allTodos[i].ID %></td>
        <td><%= allTodos[i].title %></td>
        <td><%= allTodos[i].dueDate %></td>
    </tr>
    <%} %>
</table>